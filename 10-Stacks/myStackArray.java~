public class myStackArray{
    private int[] stack;

    public myStackArray(){
	stack = new int[0];
    }
    public myStackArray(int n){
	stack = new int[1];
	stack[0] = n;
    }

    public void push(int n){
	int[] tmp = new int[stack.length + 1];
	for (int i=0;i<stack.length;i++){
	    tmp[i] = stack[i];
	} tmp[i] = n;
	stack = tmp;
    }
}
